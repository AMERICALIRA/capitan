<div class="cell">
  <div class="center">
    <h1 class="home-title thin">Recursos Laboratoria</h1>
  </div>
  
  <div class="form form-session">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-group">
        <%= f.label :email,'Correo electrónico:', :class => 'control-label' %>
        <%= f.email_field :email, autofocus: true, :class => 'form-control' %>
      </div>
    
      <div class="form-group">
        <%= f.label :password,'Contraseña:', :class => 'control-label' %>
        <%= f.password_field :password, autocomplete: "off", :class => 'form-control' %>
      </div>
    
      <% if devise_mapping.rememberable? -%>
        <div class="checkbox">
          <label>
            <%= f.check_box :remember_me %> Permanecer logueado
          </label>
        </div>
      <% end -%>
    
      <div class="actions">
        <%= f.submit "Ingresar", :class => 'btn btn-primary' %>
      </div>
    <% end %>
  </div>
</div>